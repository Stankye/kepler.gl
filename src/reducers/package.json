  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "babel",
    "es6",
    "react",
    "webgl",
    "visualization",
    "deck.gl"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/keplergl/kepler.gl.git"
